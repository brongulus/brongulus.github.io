@media (prefers-color-scheme: dark) {
  :root {
    --bg: radial-gradient(circle, #000 0%, #08000d 100%);
    --gradient: linear-gradient(-70deg, #a2facf 0%, #64acff 100%);
    --bg-sel: #0e001c;
    --link: #88c0d0;
    --font: #c9cdd7;
    --scroll: #fff;
    --blend: lighten;
    --filter: invert(100%);
    --button-icon: var(--icon-sun);
    --icon-filter: var(--icon-sun-filter); } }

[data-theme="dark"] {
  --bg: radial-gradient(circle, #000 0%, #08000d 100%);
  --bg-sel: #0e001c;
  --link: #88c0d0;
  --font: #c9cdd7;
  --scroll: #fff;
  --gradient: linear-gradient(-70deg, #a2facf 0%, #64acff 100%);
  --blend: lighten;
  --filter: invert(100%);
  --button-icon: var(--icon-sun);
  --icon-filter: var(--icon-sun-filter); }

[data-theme="light"] {
  --filter: invert(0%);
  --blend: darken;
  --gradient: linear-gradient(-70deg, #64acff 0%, slateblue 100%);
  --link: #458588;
  --scroll: #555;
  --font: #555;
  --button-icon: var(--icon-moon);
  --icon-filter: var(--icon-moon-filter);
  --bg: linear-gradient(90deg, #ebe4e1 0%, #ebe4e1 100%);
  --bg-sel: #ebe4e1; }

