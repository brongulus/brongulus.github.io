:root {
  --filter: invert(0%);
  --blend: darken;
  --inv-gradient: linear-gradient(-70deg, #a2facf 0%, #64acff 100%);
  --gradient: linear-gradient(-70deg, #64acff 0%, slateblue 100%);
  --link: #458588;
  --scroll: #555;
  --font: #555;
  --bg: linear-gradient(90deg, #ebe4e1 0%, #ebe4e1 100%);
  --bg-sel: #ebe4e1; }

@media (prefers-color-scheme: light) {
  :root {
    --filter: invert(0%);
    --blend: darken;
    --gradient: linear-gradient(-70deg, #64acff 0%, slateblue 100%);
    --inv-gradient: linear-gradient(-70deg, #a2facf 0%, #64acff 100%);
    --link: #458588;
    --scroll: #555;
    --font: #555;
    --bg: linear-gradient(90deg, #ebe4e1 0%, #ebe4e1 100%);
    --bg-sel: #ebe4e1; } }

@media (prefers-color-scheme: dark) {
  :root {
    --bg: radial-gradient(circle, #17002e 0%, #08000d 100%);
    --gradient: linear-gradient(-70deg, #a2facf 0%, #64acff 100%);
    --inv-gradient: linear-gradient(-70deg, #64acff 0%, slateblue 100%);
    --bg-sel: #0e001c;
    --link: #88c0d0;
    --font: #c9cdd7;
    --scroll: #fff;
    --blend: lighten;
    --filter: invert(100%); } }

[data-theme="dark"] {
  --bg: radial-gradient(circle, #17002e 0%, #08000d 100%);
  --bg-sel: #0e001c;
  --link: #88c0d0;
  --font: #c9cdd7;
  --scroll: #fff;
  --gradient: linear-gradient(-70deg, #a2facf 0%, #64acff 100%);
  --inv-gradient: linear-gradient(-70deg, #64acff 0%, slateblue 100%);
  --blend: lighten;
  --filter: invert(100%); }

[data-theme="light"] {
  --filter: invert(0%);
  --blend: darken;
  --gradient: linear-gradient(-70deg, #64acff 0%, slateblue 100%);
  --inv-gradient: linear-gradient(-70deg, #a2facf 0%, #64acff 100%);
  --link: #458588;
  --scroll: #555;
  --font: #555;
  --bg: linear-gradient(90deg, #ebe4e1 0%, #ebe4e1 100%);
  --bg-sel: #ebe4e1; }

* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  line-height: 1.5;
  box-sizing: border-box; }

@font-face {
  font-family: "Cormorant";
  font-weight: 300;
  font-style: italic;
  font-display: fallback; }

@font-face {
  font-family: "Josefin Sans";
  font-weight: 200;
  font-display: fallback; }

@font-face {
  font-family: "Roboto Mono";
  font-weight: 300;
  font-display: fallback; }

@font-face {
  font-family: "Inter";
  font-weight: 300;
  font-display: fallback; }

@font-face {
  font-family: "Inter";
  font-weight: 700;
  font-display: fallback; }

@font-face {
  font-family: "Source Serif Pro";
  font-weight: 200;
  font-style: italic;
  font-display: fallback; }

@font-face {
  font-family: kepler-std-semicondensed-dis, serif;
  font-weight: 700;
  font-style: italic; }

@font-face {
  font-family: "Bodoni Moda", serif;
  font-style: italic;
  font-weight: 500; }

html,
.content {
  display: block;
  color: var(--font);
  background: var(--bg);
  margin: 0;
  height: 100%;
  transition: background 0.3s ease; }

html {
  color: var(--bg);
  font-family: "Inter", "Helvetica Neue", "Segoe UI", Helvetica, Arial, sans-serif;
  font-size: 22px;
  overflow-y: hidden;
  overflow-x: hidden;
  margin: 0; }
  @media (max-width: 625px) {
    html {
      font-size: 18px; } }
body {
  margin: 0;
  overflow: hidden;
  -webkit-text-size-adjust: 100%; }

.container {
  margin: 0 auto;
  overflow: scroll;
  max-width: 1000px;
  width: 90%;
  transition: background 0.3s ease; }

h1 {
  color: var(--font);
  font-weight: normal;
  margin-top: 4rem;
  margin-bottom: 1.5rem;
  font-size: 3.2rem;
  line-height: 1; }

h2 {
  font-family: "Josefin Sans", sans-serif;
  color: var(--font);
  margin-top: 2.1rem;
  margin-bottom: 1.4rem;
  font-size: 2.4rem;
  line-height: 1; }

h3 {
  text-transform: uppercase;
  font-family: "Josefin Sans", sans-serif;
  font-size: 1.6rem; }

h4,
h5,
h6 {
  font-family: "Inter", "Helvetica Neue", "Segoe UI", Helvetica, Arial, sans-serif;
  line-height: normal; }

a {
  color: var(--link);
  text-decoration: none; }

blockquote {
  display: block;
  margin-block-start: 1em;
  margin-block-end: 1em;
  margin-inline-start: 4rem;
  margin-inline-end: 4rem;
  border-bottom: 3px double var(--font);
  border-top: 3px double var(--font); }

img {
  display: block;
  margin: 0 0 1rem;
  filter: var(--filter);
  mix-blend-mode: var(--blend);
  max-width: 100%; }
  img:hover, img:focus {
    opacity: 0.8; }

figcaption {
  font-style: italic;
  color: #aaa; }

::-moz-selection {
  /* Code for Firefox */
  color: var(--bg-sel);
  background: var(--font); }

::selection {
  color: var(--bg-sel);
  background: var(--font); }

thead {
  background-color: var(--font);
  color: var(--bg-sel);
  font-size: 1.15rem;
  letter-spacing: 2%; }
  thead ::-moz-selection {
    /* Code for Firefox */
    color: var(--font);
    background: var(--bg-sel); }
  thead ::selection {
    color: var(--font);
    background: var(--bg-sel); }

td {
  vertical-align: top;
  text-align: center;
  padding: 0 10px; }

table {
  table-layout: fixed;
  width: 100%;
  border-spacing: 0;
  padding-bottom: 20px; }

th,
td {
  border: 1px solid var(--font); }

th:first-of-type {
  border-top-left-radius: 10px; }

th:last-of-type {
  border-top-right-radius: 10px; }

tr:last-of-type td:first-of-type {
  border-bottom-left-radius: 10px; }

tr:last-of-type td:last-of-type {
  border-bottom-right-radius: 10px; }

.custom-scrollbar {
  display: block;
  overflow: auto;
  height: 100vh; }

.custom-scrollbar::-webkit-scrollbar {
  background-color: transparent;
  width: 20px; }

.custom-scrollbar::-webkit-scrollbar-track {
  background-color: transparent; }

.custom-scrollbar::-webkit-scrollbar-thumb {
  background: var(--scroll);
  border-radius: 20px;
  border: 6px solid transparent;
  background-clip: content-box; }

.custom-scrollbar::-webkit-scrollbar-thumb:hover {
  background-color: #a8bbbf; }

.toggle {
  display: none; }

.label {
  display: inline-block;
  align-items: right;
  position: fixed;
  left: auto;
  bottom: auto;
  right: 2.5vw;
  top: 1.2rem;
  background: var(--font);
  border-radius: 5rem;
  padding: 0.5rem;
  margin: 1rem 0 0 1rem;
  height: 2rem;
  width: 2rem;
  font-size: 1.5rem;
  transition: background 0.5s ease;
  cursor: pointer;
  z-index: 1; }
  @media only screen and (max-width: 850px) {
    .label {
      transform: scale(0.92);
      top: 0.5rem; } }
  @media only screen and (max-width: 625px) {
    .label {
      transform: scale(0.85);
      top: 0.1rem; } }
.themebox {
  color: var(--font); }

.themebox::after {
  content: "Night"; }

.toggle:checked ~ .themebox::after {
  content: "Day"; }

.label .ball {
  position: absolute;
  background: var(--bg);
  border-radius: 50%;
  top: 0.25rem;
  left: 0.25rem;
  height: 1.5rem;
  width: 0.75rem;
  border-radius: 0.75rem 0 0 0.75rem;
  transition: transform 0.4s ease, background-color 0.5s ease; }

.toggle:checked + .content .label .ball {
  display: inline-block;
  transform: translateX(0.75rem) rotate(180deg); }

pre,
code {
  font-family: "Roboto Mono", monospaced; }

code {
  border-radius: 0px;
  color: #bf616a;
  font-size: 100%;
  overflow-x: auto; }

pre {
  background: var(--bg);
  border-radius: 0.5rem;
  margin: 0 0 1rem; }

pre code {
  color: var(--font);
  font-size: 100%;
  padding: 0.01rem; }

.highlight {
  background: var(--gradient);
  border-radius: 0.6rem;
  margin: 0 0 1rem;
  padding: 0.1rem; }
  .highlight pre {
    margin-bottom: 0;
    overflow-x: auto; }

::-webkit-scrollbar {
  background-color: transparent;
  width: 15px; }

::-webkit-scrollbar-thumb {
  background: var(--scroll);
  border-radius: 20px;
  border: 6px solid transparent;
  background-clip: content-box; }

.post {
  padding: 3rem 0; }
  .post-info {
    color: #aaa;
    font-family: Palatino, "Palatino LT STD", "Palatino Linotype", "Book Antiqua", "Georgia", serif;
    letter-spacing: 0.5px;
    text-align: center; }
    .post-info span {
      font-style: italic; }
  .post blockquote p {
    font-family: "Source Serif Pro", serif;
    font-style: italic;
    font-weight: 300;
    font-size: 1.4em;
    line-height: 1.35em;
    margin: 0.4em auto;
    padding: 0.25em 0;
    text-align: center;
    quotes: none; }
  .post-title {
    color: var(--font);
    font-family: "Cormorant", serif;
    font-style: italic;
    font-size: 4.5rem;
    margin: 1rem 0;
    text-align: center; }
  .post-line {
    border-top: 0.4rem solid var(--font);
    display: block;
    margin: 0 auto 3rem;
    width: 4rem; }
  .post p {
    font-family: "Inter", sans;
    margin: 0 0 1rem;
    text-align: justify; }
  .post a:hover {
    text-decoration: underline; }
  .post img {
    margin: 0 auto 0.5rem; }
  .post img {
    color: #aaa;
    display: block;
    font-family: "Inter", "Helvetica Neue", "Segoe UI", Helvetica, Arial, sans-serif;
    font-size: 0.9rem;
    font-style: normal;
    text-align: center; }
  .post em {
    font-style: italic; }
  .post img.emoji {
    display: inline-block;
    left: 0;
    transform: none;
    width: 1rem;
    height: 1rem;
    vertical-align: text-top;
    padding: 0;
    margin: 0; }

