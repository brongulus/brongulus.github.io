#TableOfContents {
  font-size: 1.2rem;
  padding-top: 0em;
  padding-left: 1em;
  margin-top: 0em;
  font-family: "Source Sans Pro", sans-serif; }
  @media (min-width: 1480px) {
    #TableOfContents {
      float: right;
      clear: right;
      margin-right: -50%;
      width: 40%;
      margin-top: 0.3rem;
      margin-bottom: 0;
      padding-bottom: 0.1rem;
      line-height: 1.3;
      vertical-align: baseline;
      position: sticky;
      top: 1rem; } }
  @media (max-width: 700px) {
    #TableOfContents {
      color: var(--font);
      font-size: 120%; } }
  #TableOfContents a:hover {
    color: var(--bg-sel);
    background: var(--link);
    text-decoration: none; }

#TableOfContents:before {
  content: "Table of Contents";
  border-bottom: 1px solid var(--font);
  font-family: "Josefin Sans", sans-serif;
  font-weight: 600;
  display: block;
  font-size: 130%;
  padding-top: 2em;
  margin-top: -2em;
  margin-left: -1em;
  text-transform: capitalize; }
  @media (max-width: 1480px) {
    #TableOfContents:before {
      margin-left: -0.75em; } }
#TableOfContents ul {
  padding: 0;
  font-weight: 400;
  font-size: 87%;
  list-style: none;
  counter-reset: list 0; }

#TableOfContents ul ul {
  padding-left: 0em;
  font-weight: 300;
  line-height: 1.5em;
  margin-top: 0em;
  margin-bottom: 0.3em;
  padding-left: 2em; }

#TableOfContents h2:before {
  content: "";
  counter-reset: section; }

#TableOfContents ul li {
  vertical-align: top;
  font-size: 95%;
  display: inline-block;
  width: 32%; }
  @media (min-width: 1480px) {
    #TableOfContents ul li {
      width: 100%; } }
  @media (max-width: 700px) {
    #TableOfContents ul li {
      width: 100%;
      font-size: 100%; } }
#TableOfContents ul li:before {
  display: inline-block;
  counter-increment: list;
  content: counters(list, ".") ".";
  width: 2em;
  margin-left: -2em;
  text-align: right;
  text-transform: uppercase;
  color: var(--link); }

#TableOfContents ul li ul,
#TableOfContents ul li ul li {
  display: static;
  width: 100%;
  padding-left: 0;
  line-height: 1.35em;
  font-size: 95%;
  text-transform: capitalize; }
  @media (max-width: 700px) {
    #TableOfContents ul li ul,
    #TableOfContents ul li ul li {
      text-transform: uppercase; } }
#TableOfContents ul li ul li:before {
  content: ""; }

