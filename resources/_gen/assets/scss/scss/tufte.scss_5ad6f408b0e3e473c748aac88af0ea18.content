html,
.content {
  display: block;
  color: var(--font);
  background: var(--bg);
  margin: 0;
  height: 100%;
  transition: background 0.3s ease; }

html {
  color: var(--bg);
  font-family: "Inter", "Helvetica Neue", "Segoe UI", Helvetica, Arial, sans-serif;
  font-size: 22px;
  overflow: scroll;
  overflow-y: hidden;
  overflow-x: hidden;
  margin: 0; }
  @media (max-width: 625px) {
    html {
      font-size: 18px; } }
body {
  margin: 0;
  overflow: hidden;
  -webkit-text-size-adjust: 100%; }

.container {
  margin: 0 auto;
  max-width: 1000px;
  width: 90%;
  transition: background 0.3s ease; }
  @media (min-width: 1100px) {
    .container {
      padding-right: 20%;
      width: 87.5%;
      max-width: 1250px; } }
h1 {
  color: var(--font);
  font-weight: normal;
  margin-top: 4rem;
  margin-bottom: 1.5rem;
  font-size: 3.2rem;
  line-height: 1; }

h2 {
  font-family: "Josefin Sans", sans-serif;
  color: var(--font);
  margin-top: 2.1rem;
  margin-bottom: 1.4rem;
  font-size: 2.4rem;
  line-height: 1; }

h3 {
  text-transform: uppercase;
  font-family: "Josefin Sans", sans-serif;
  font-size: 1.6rem; }

h4,
h5,
h6 {
  font-family: "Inter", "Helvetica Neue", "Segoe UI", Helvetica, Arial, sans-serif;
  line-height: normal; }

a {
  color: var(--link);
  text-decoration: none; }

blockquote {
  display: block;
  margin-inline-end: 4rem;
  border-bottom: 3px double var(--font);
  border-top: 3px double var(--font); }

img {
  display: block;
  margin: 0 0 1rem;
  filter: var(--filter);
  mix-blend-mode: var(--blend);
  max-width: 100%; }
  img:hover, img:focus {
    opacity: 0.8; }

figcaption {
  font-style: italic;
  color: #aaa; }

::-moz-selection {
  /* Code for Firefox */
  color: var(--bg-sel);
  background: var(--font); }

::selection {
  color: var(--bg-sel);
  background: var(--font); }

thead {
  background-color: var(--font);
  color: var(--bg-sel);
  font-size: 1.15rem;
  letter-spacing: 2%; }
  thead ::-moz-selection {
    /* Code for Firefox */
    color: var(--font);
    background: var(--bg-sel); }
  thead ::selection {
    color: var(--font);
    background: var(--bg-sel); }

td {
  vertical-align: top;
  text-align: center;
  padding: 0 10px; }

table {
  table-layout: fixed;
  width: 100%;
  border-spacing: 0;
  padding-bottom: 20px; }

th,
td {
  border: 1px solid var(--font); }

th:first-of-type {
  border-top-left-radius: 10px; }

th:last-of-type {
  border-top-right-radius: 10px; }

tr:last-of-type td:first-of-type {
  border-bottom-left-radius: 10px; }

tr:last-of-type td:last-of-type {
  border-bottom-right-radius: 10px; }

.custom-scrollbar {
  display: block;
  overflow: auto;
  height: 100vh; }

.custom-scrollbar::-webkit-scrollbar {
  background-color: transparent;
  width: 20px; }

.custom-scrollbar::-webkit-scrollbar-track {
  background-color: transparent; }

.custom-scrollbar::-webkit-scrollbar-thumb {
  background: var(--scroll);
  border-radius: 20px;
  border: 6px solid transparent;
  background-clip: content-box; }
  .custom-scrollbar::-webkit-scrollbar-thumb:hover {
    opacity: 0.6; }

.custom-scrollbar::-webkit-scrollbar-corner {
  background: var(--bg); }

.toggle {
  display: none; }

.label {
  display: inline-block;
  align-items: right;
  position: fixed;
  left: auto;
  bottom: auto;
  right: 2.5vw;
  top: 1.2rem;
  background: var(--font);
  border-radius: 5rem;
  padding: 0.5rem;
  margin: 1rem 0 0 1rem;
  height: 2rem;
  width: 2rem;
  font-size: 1.5rem;
  transition: background 0.5s ease;
  cursor: pointer;
  z-index: 1; }
  @media only screen and (max-width: 850px) {
    .label {
      transform: scale(0.92);
      top: 0.5rem; } }
  @media only screen and (max-width: 625px) {
    .label {
      transform: scale(0.85);
      top: 0.1rem; } }
.themebox {
  color: var(--font); }

.themebox::after {
  content: "Night"; }

.toggle:checked ~ .themebox::after {
  content: "Day"; }

.label .ball {
  position: absolute;
  background: var(--bg);
  border-radius: 50%;
  top: 0.25rem;
  left: 0.25rem;
  height: 1.5rem;
  width: 0.75rem;
  border-radius: 0.75rem 0 0 0.75rem;
  transition: transform 0.4s ease, background-color 0.5s ease; }

.toggle:checked + .content .label .ball {
  display: inline-block;
  transform: translateX(0.75rem) rotate(180deg); }

.toggle-icon {
  background: var(--button-icon);
  width: 1.2em;
  height: 1.2em;
  margin: 0 0 0 0.5em;
  transform: translateY(1px);
  /* Optical adjustment */
  filter: var(--icon-filter);
  color: var(--font);
  transition: filter 300ms ease-in-out; }

pre,
code {
  font-family: "Roboto Mono", monospaced; }

code {
  border-radius: 0px;
  color: var(--font);
  font-style: italic;
  font-size: 100%;
  overflow-x: auto; }

pre {
  background: var(--bg);
  border-radius: 0.5rem;
  margin: 0 0 1rem; }

pre code:before {
  position: relative;
  top: -2px;
  left: -2px;
  display: inline-flex;
  padding: 0.25em 0.5em;
  background: var(--gradient);
  border-bottom-right-radius: 0.6rem;
  border: 0;
  color: var(--bg-sel);
  font-size: 80%;
  font-weight: 600; }

pre code {
  color: var(--font);
  font-size: 100%;
  font-style: normal;
  padding: 0.01rem; }

.highlight {
  background: var(--gradient);
  border-radius: 0.6rem;
  margin: 0 0 1rem;
  padding: 0.1rem; }
  .highlight pre {
    margin-bottom: 0;
    overflow-x: auto; }

::-webkit-scrollbar {
  background-color: transparent;
  width: 15px; }

::-webkit-scrollbar-thumb {
  background: var(--scroll);
  border-radius: 20px;
  border: 6px solid transparent;
  background-clip: content-box; }

